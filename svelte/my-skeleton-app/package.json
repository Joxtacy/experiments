    "vite": "^5.0.3",
    "vitest": "^2.0.0",
    "postcss": "8.4.49",
    "autoprefixer": "10.4.20",
    "tailwindcss": "3.4.17",
    "@skeletonlabs/skeleton": "2.10.4",
